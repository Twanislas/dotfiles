Host *.home
  ProxyCommand ssh bastion.twan.ovh -W %h:%p
  RemoteForward 52698 localhost:52698

Host rx01.home
  IdentityFile ~/.ssh/id_rsa

Host pve01.home pvr.home
  User root

Host *.github.com github.com
  User git
  ForwardX11 no
  ForwardAgent no

Host *
  Protocol 2
  Compression yes
  ServerAliveInterval 5
  ForwardAgent yes
  ForwardX11 yes
  CheckHostIP yes
  VerifyHostKeyDNS yes
