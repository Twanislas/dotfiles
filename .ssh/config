Host *
  Protocol 2
  Compression yes
  ServerAliveInterval 5
  ForwardAgent yes
  CheckHostIP yes
  VerifyHostKeyDNS yes

# GitHub config
Host *.github.com github.com
  Hostname ssh.github.com
  Port 443
  User git
  ForwardX11 no
  ForwardAgent no
  PermitLocalCommand no

# Home SSH proxy
Match Host *.i,*.prod,*.iot,*.lan !exec "nslookup %h"
  ProxyJump home.twan.ovh:222

# HomeAssistant
Host hassio.iot
  User root

# Ubiquiti devices
Host rx*.i sw*.i ap*.i
  User admin

# Bookmarks
Host srvtmp.prod

# Include specific configs
Include ~/.ssh/config.*