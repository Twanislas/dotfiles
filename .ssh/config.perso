# GitHub config
Host *.github.com github.com
  Hostname ssh.github.com
  Port 443
  User git
  ForwardX11 no
  ForwardAgent no
  PermitLocalCommand no

# Home SSH proxy
Match Host *.i,*.prod,*.iot,*.lan !exec "nslookup %h"
  ProxyJump home.twan.ovh:222

# HomeAssistant
Host hassio.iot
  User root

# USG
Host rx01.i
  User admin